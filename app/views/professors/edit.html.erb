<h1> Professor Create Form</h1>
<% @professor.errors.full_messages.each do |msg|%>
<%= msg %>
<% end %>
<%= form_for(@professor) do |p| %>
    <%= p.label :name %>
    <%= p.text_field :name %>
    <%= p.label :department %>
    <%= p.text_field :department %>
    <%= p.label:level %>
    <%= p.text_field :level %>

    <%= p.submit %>
<%end%>